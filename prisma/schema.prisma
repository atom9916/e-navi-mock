generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model State {
  id        Int       @id @default(autoincrement())
  name      String
}

model Attendance {
  id        Int       @id @default(autoincrement())
  name      String    
}

model Tardiness {
  id        Int       @id @default(autoincrement())
  name      String
}
